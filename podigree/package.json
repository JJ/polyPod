{
  "name": "@polypoly-eu/podigree",
  "version": "0.6.2",
  "description": "An assortment of Pod implementations of noble descent",
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/polyPod.git"
  },
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "rollup -c && npm run test:electron && npm run test:node",
    "test:electron": "electron-mocha --renderer --require ts-node/register src/tests/electron/*.test.ts",
    "test:node": "ts-mocha src/tests/node/*.test.ts",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "dependencies": {
    "@polypoly-eu/aop-ts": "file:../aop-ts",
    "@polypoly-eu/bubblewrap": "file:../bubblewrap",
    "@polypoly-eu/fetch-spec": "file:../fetch-spec",
    "@polypoly-eu/poly-api": "file:../poly-api",
    "@polypoly-eu/port-authority": "file:../port-authority",
    "@polypoly-eu/postoffice": "file:../postoffice",
    "@polypoly-eu/rdf": "file:../rdf",
    "@polypoly-eu/rdf-convert": "file:../rdf-convert",
    "@rdfjs/dataset": "^1.0.1",
    "@types/rdf-js": "^4.0.0",
    "fp-ts": "^2.8.2",
    "io-ts": "2.2.10",
    "memfs": "^3.2.0"
  },
  "devDependencies": {
    "@polypoly-eu/eslint-config-polypoly": "file:../eslint-config-polypoly",
    "@polypoly-eu/rdf-spec": "file:../rdf-spec",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.1.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/connect": "^3.4.34",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.21",
    "@types/node-fetch": "^2.5.8",
    "@types/rdfjs__dataset": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^4.1.0",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^2.1.0",
    "connect": "^3.7.0",
    "electron": "^11.2.0",
    "electron-mocha": "^10.0.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fast-check": "^2.11.0",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "prettier": "^2.2.1",
    "rollup": "^2.37.0",
    "ts-mocha": "^8.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "peerDependencies": {
    "body-parser": "^1.19.0"
  },
  "private": true
}
