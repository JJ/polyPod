{
  "name": "@polypoly-eu/bubblewrap",
  "version": "0.1.0",
  "description": "Lossless encoding and decoding of objects into byte arrays",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "benchmark": "./bin/run-benchmarks.sh",
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && npm run rollup",
    "rollup": "rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/bubblewrap.git"
  },
  "author": "Lars Hupel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/polypoly-eu/bubblewrap/issues"
  },
  "homepage": "https://github.com/polypoly-eu/bubblewrap#readme",
  "devDependencies": {
    "@polypoly-eu/rdf": "0.1.0",
    "@polypoly-eu/rdf-spec": "0.1.0",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-sucrase": "^3.0.2",
    "@types/benchmark": "^1.0.33",
    "@types/jest": "^25.2.3",
    "@types/n3": "^1.1.6",
    "@types/rdf-js": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "benchmark": "^2.1.4",
    "chokidar-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "fast-check": "^1.24.2",
    "jest": "^25.5.4",
    "n3": "^1.3.7",
    "rollup": "^2.15.0",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@msgpack/msgpack": "^1.12.2"
  }
}
