    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/bubblewrap.git"
  },
  "author": "Lars Hupel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/polypoly-eu/bubblewrap/issues"
  },
  "homepage": "https://github.com/polypoly-eu/bubblewrap#readme",
  "devDependencies": {
    "@polypoly-eu/rdf": "0.0.3",
    "@polypoly-eu/rdf-spec": "0.0.8",
    "@rollup/plugin-sucrase": "^3.0.1",
    "@types/jest": "^25.2.2",
    "@types/rdf-js": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "fast-check": "^1.24.2",
    "jest": "^25.5.4",
    "rollup": "^2.10.2",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.17.6",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@msgpack/msgpack": "^1.12.2"
  }
