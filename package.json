{
  "name": "@polypoly-eu/rdf-spec",
  "version": "0.3.1",
  "description": "Standalone executable spec for RDFJS",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "npm run test:jest && npm run test:mocha && npm run test:jasmine",
    "test:jest": "jest",
    "test:mocha": "ts-mocha src/tests/**.test.ts",
    "test:jasmine": "ts-node node_modules/.bin/jasmine --config=jasmine.json",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/rdf-spec.git"
  },
  "author": "Lars Hupel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polypoly-eu/rdf-spec/issues"
  },
  "homepage": "https://github.com/polypoly-eu/rdf-spec#readme",
  "devDependencies": {
    "@graphy/core.data.factory": "^4.3.3",
    "@graphy/memory.dataset.fast": "^4.3.3",
    "@polypoly-eu/eslint-config-polypoly": "0.0.4",
    "@rdfjs/data-model": "^1.1.3",
    "@rdfjs/dataset": "^1.0.1",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/chai": "^4.2.14",
    "@types/jest": "^26.0.15",
    "@types/n3": "^1.4.4",
    "@types/node": "^14.14.10",
    "@types/rdfjs__dataset": "^1.0.4",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.14.0",
    "jasmine": "^3.6.3",
    "jest": "^26.6.3",
    "mocha": "^8.2.1",
    "n3": "^1.6.4",
    "prettier": "^2.2.0",
    "rdf-data-factory": "^1.0.4",
    "rollup": "^2.33.3",
    "ts-jest": "^26.4.4",
    "ts-mocha": "^8.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  },
  "dependencies": {
    "@types/rdf-js": "^4.0.0",
    "chai": "^4.2.0",
    "fast-check": "^2.2.1"
  }
}
