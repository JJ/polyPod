    "faucet-pipeline-static": "^1.2.0",
    "faucet-pipeline-typescript": "^2.1.6",
    "typescript": "3.9.7"
  },
  "engines": {
    "polypoly": "^0.4.2"
  },
  "polypoly": {
    "root": "dist"
  },
  "dependencies": {
    "@polypoly-eu/eslint-config-polypoly": "0.0.1",
    "@polypoly-eu/feature-bootstrap": "0.0.1",
    "@polypoly-eu/stylelint-config-polypoly": "0.0.1",
    "eslint": "^7.5.0",
    "i18next": "^19.5.2",
    "prettier": "^2.0.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-i18next": "^11.7.0",
