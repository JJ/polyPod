{
  "name": "@polypoly-eu/rdf",
  "version": "0.0.3",
  "description": "TypeScript implementation of the RDF Data Model spec",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/rdf.git"
  },
  "author": "Lars Hupel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polypoly-eu/rdf/issues"
  },
  "homepage": "https://github.com/polypoly-eu/rdf#readme",
  "dependencies": {
    "@types/rdf-js": "^3.0.0"
  },
  "devDependencies": {
    "@polypoly-eu/rdf-spec": "0.0.8",
    "@rdfjs/data-model": "^1.1.2",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@types/jest": "^25.2.1",
    "@typescript-eslint/eslint-plugin": "^2.30.0",
    "@typescript-eslint/parser": "^2.30.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.5.2",
    "rollup": "^2.7.5",
    "ts-jest": "^25.4.0",
    "typedoc": "^0.17.6",
    "typescript": "^3.7.5"
  }
}
