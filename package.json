{
  "name": "@polypoly-eu/postoffice",
  "version": "0.1.0",
  "description": "RPC library for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/postoffice.git"
  },
  "author": "Lars Hupel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/polypoly-eu/postoffice/issues"
  },
  "homepage": "https://github.com/polypoly-eu/postoffice#readme",
  "devDependencies": {
    "@rollup/plugin-sucrase": "^3.0.2",
    "@types/jest": "^26.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "chokidar-cli": "^2.1.0",
    "eslint": "^6.8.0",
    "jest": "^25.5.4",
    "rollup": "^2.17.1",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.17.7",
    "typescript": "^3.9.5"
  }
