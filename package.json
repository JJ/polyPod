{
  "name": "@polypoly-eu/rdf",
  "version": "0.2.0",
  "description": "TypeScript implementation of the RDF Data Model spec",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/rdf.git"
  },
  "author": "Lars Hupel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polypoly-eu/rdf/issues"
  },
  "homepage": "https://github.com/polypoly-eu/rdf#readme",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "dependencies": {
    "@types/rdf-js": "^4.0.0"
  },
  "devDependencies": {
    "@polypoly-eu/eslint-config-polypoly": "0.0.4",
    "@polypoly-eu/rdf-spec": "^0.3.1",
    "@rdfjs/data-model": "^1.2.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/jest": "^26.0.16",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.15.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rollup": "^2.34.2",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.5"
  }
}
