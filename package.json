{
  "name": "@polypoly-eu/podigree",
  "version": "0.6.2",
  "description": "An assortment of Pod implementations of noble descent",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "rollup -c && npm run test:electron && npm run test:node",
    "test:electron": "electron-mocha --renderer --require ts-node/register src/tests/electron/*.test.ts",
    "test:node": "ts-mocha src/tests/node/*.test.ts",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/podigree.git"
  },
  "author": "Lars Hupel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polypoly-eu/podigree/issues"
  },
  "homepage": "https://github.com/polypoly-eu/podigree#readme",
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "dependencies": {
    "@polypoly-eu/aop-ts": "0.0.1",
    "@polypoly-eu/bubblewrap": "^0.1.2",
    "@polypoly-eu/poly-api": "^0.7.2",
    "@polypoly-eu/port-authority": "^0.2.1",
    "@polypoly-eu/postoffice": "^0.1.0",
    "@polypoly-eu/rdf": "^0.2.0",
    "@polypoly-eu/rdf-convert": "^0.1.0",
    "@rdfjs/dataset": "^1.0.1",
    "@types/rdf-js": "^4.0.0",
    "fp-ts": "^2.8.2",
    "io-ts": "2.2.10",
    "memfs": "^3.2.0"
  },
  "peerDependencies": {
    "body-parser": "^1.19.0"
  },
  "devDependencies": {
    "@polypoly-eu/eslint-config-polypoly": "0.0.4",
    "@polypoly-eu/rdf-spec": "^0.3.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/connect": "^3.4.33",
    "@types/mocha": "^8.0.3",
    "@types/node": "^14.11.2",
    "@types/node-fetch": "^2.5.7",
    "@types/rdfjs__dataset": "^1.0.4",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^2.1.0",
    "connect": "^3.7.0",
    "electron": "^10.1.2",
    "electron-mocha": "^9.1.0",
    "eslint": "^7.10.0",
    "fast-check": "^2.3.0",
    "mocha": "^8.1.3",
    "node-fetch": "^2.6.1",
    "prettier": "^2.1.2",
    "rollup": "^2.28.2",
    "ts-mocha": "^7.0.0",
    "typedoc": "^0.19.2",
    "typescript": "^4.0.3"
  }
}
