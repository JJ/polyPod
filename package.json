{
  "name": "@polypoly-eu/rdf-convert",
  "version": "0.0.1",
  "description": "Converts terms and quads between different RDFJS-compliant data factories",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/rdf-convert.git"
  },
  "author": "Lars Hupel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/polypoly-eu/rdf-convert/issues"
  },
  "files": [
    "dist/*.ts",
    "dist/*.js"
  ],
  "homepage": "https://github.com/polypoly-eu/rdf-convert#readme",
  "devDependencies": {
    "@graphy/core.data.factory": "^4.2.1",
    "@graphy/memory.dataset.fast": "^4.2.1",
    "@polypoly-eu/eslint-config-polypoly": "0.0.1",
    "@polypoly-eu/rdf-spec": "^0.2.0",
    "@rdfjs/data-model": "^1.1.2",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/jest": "^26.0.10",
    "@types/n3": "^1.4.3",
    "@types/rdfjs__dataset": "^1.0.3",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.7.0",
    "fast-check": "^2.2.0",
    "jest": "^25.5.4",
    "n3": "^1.6.2",
    "prettier": "^2.1.0",
    "rollup": "^2.26.4",
    "ts-jest": "^25.5.1",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7"
  },
  "dependencies": {
    "@types/rdf-js": "^3.0.3"
  }
}
