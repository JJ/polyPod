{
  "name": "@polypoly-eu/postoffice",
  "version": "0.0.1",
  "description": "Post Office transmits function calls across iframes",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "tsc --noEmit && npm run test:browser && npm run test:node",
    "test:browser": "rollup -c ./karma/rollup.config.js && karma start",
    "test:node": "ts-node ./node_modules/jasmine/bin/jasmine --config=jasmine.json"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/postoffice.git"
  },
  "author": "Lars Hupel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/polypoly-eu/postoffice/issues"
  },
  "homepage": "https://github.com/polypoly-eu/postoffice#readme",
  "devDependencies": {
    "@polypoly-eu/rdf": "0.0.1",
    "@polypoly-eu/rdf-spec": "0.0.2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-sucrase": "^3.0.0",
    "@types/body-parser": "^1.19.0",
    "@types/express": "^4.17.3",
    "@types/jasmine": "^3.5.8",
    "@types/node": "^13.9.0",
    "@types/promise-timeout": "^1.3.0",
    "@types/rdf-js": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "body-parser": "^1.19.0",
    "eslint": "^6.8.0",
    "express": "^4.17.1",
    "fast-check": "^1.23.0",
    "jasmine": "^3.5.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^3.1.1",
    "karma-rollup-preprocessor": "^7.0.3",
    "karma-spec-reporter": "0.0.32",
    "memfs": "^3.1.1",
