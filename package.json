{
  "name": "@polypoly-eu/bubblewrap",
  "version": "0.1.2",
  "description": "Lossless encoding and decoding of objects into byte arrays",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "benchmark": "./bin/run-benchmarks.sh",
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && npm run rollup",
    "rollup": "rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "jest",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/bubblewrap.git"
  },
  "author": "Lars Hupel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/polypoly-eu/bubblewrap/issues"
  },
  "homepage": "https://github.com/polypoly-eu/bubblewrap#readme",
  "files": [
    "dist/*.js",
    "dist/*.d.ts"
  ],
  "devDependencies": {
    "@polypoly-eu/eslint-config-polypoly": "0.0.4",
    "@polypoly-eu/rdf": "^0.1.0",
    "@polypoly-eu/rdf-spec": "^0.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/benchmark": "^1.0.33",
    "@types/jest": "^26.0.13",
    "@types/n3": "^1.4.4",
    "@types/rdf-js": "^4.0.0",
    "benchmark": "^2.1.4",
    "chokidar-cli": "^2.1.0",
    "eslint": "^7.8.1",
    "fast-check": "^2.2.1",
    "jest": "^26.4.2",
    "n3": "^1.6.3",
    "prettier": "^2.1.1",
    "rollup": "^2.26.10",
    "ts-jest": "^26.3.0",
    "typedoc": "^0.19.1",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "@msgpack/msgpack": "^2.0.0"
  }
}
