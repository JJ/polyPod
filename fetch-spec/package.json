{
  "name": "@polypoly-eu/fetch-spec",
  "version": "0.0.1",
  "description": "Standalone executable spec for window.fetch",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "scripts": {
    "build": "rm -rf tsconfig.tsbuildinfo dist && tsc --emitDeclarationOnly && rollup -c",
    "dev": "tsc --watch --incremental",
    "eslint": "eslint src --ext .ts",
    "test": "npm-run-all test:jest test:mocha test:jasmine test:electron",
    "test:electron": "electron-mocha --renderer --require ts-node/register src/tests/browser/*.test.ts",
    "test:jest": "jest",
    "test:mocha": "ts-mocha src/tests/node/*.test.ts",
    "test:jasmine": "ts-node node_modules/.bin/jasmine --config=jasmine.json",
    "docs": "typedoc",
    "dev:docs": "chokidar 'src/**/*' -c 'npm run docs'"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/polypoly-eu/fetch-spec.git"
  },
  "author": "Lars Hupel",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/polypoly-eu/fetch-spec/issues"
  },
  "homepage": "https://github.com/polypoly-eu/fetch-spec#readme",
  "devDependencies": {
    "@polypoly-eu/eslint-config-polypoly": "0.0.2",
    "@rollup/plugin-sucrase": "^3.1.0",
    "@types/chai": "^4.2.14",
    "@types/chai-as-promised": "^7.1.3",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.6",
    "@types/node-fetch": "^2.5.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chokidar-cli": "^2.1.0",
    "electron": "^11.2.0",
    "electron-mocha": "^10.0.0",
    "eslint": "^7.18.0",
    "jasmine": "^3.6.4",
    "jest": "^25.5.4",
    "jsdom": "^16.4.0",
    "mocha": "^8.2.1",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rollup": "^2.37.0",
    "ts-jest": "^25.5.1",
    "ts-mocha": "^8.0.0",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typescript": "^3.9.7"
  },
  "peerDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1"
  },
  "files": [
    "dist/*.ts",
    "dist/*.js"
  ]
}
